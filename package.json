{
    "name": "loan-lead-management",
    "version": "1.0.0",
    "description": "Loan lead management system that ingests, deduplicates, and distributes leads to lenders",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "typecheck": "tsc --noEmit",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "format": "biome format --write ."
    },
    "keywords": [
        "loan",
        "lead",
        "management",
        "api"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.6.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.2.1",
        "typeorm": "^0.3.19",
        "winston": "^3.11.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.6",
        "@types/pg": "^8.10.9",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
